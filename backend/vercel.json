{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "routes": [
    {
      "src": "/test",
      "dest": "api/test.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 300
    },
    "api/test.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}

